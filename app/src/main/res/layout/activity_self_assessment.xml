<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".self_assessment">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="233dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="Self Assessment"
        android:textColor="@color/black"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="89dp"
        tools:layout_editor_absoluteY="58dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Do you have any of the below symptoms?"
        android:textColor="@color/black"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="125dp"
        tools:ignore="MissingConstraints"/>

    <RadioButton
        android:id="@+id/radioVeryfrequent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Very frequent "
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="646dp"
        tools:ignore="MissingConstraints"/>

    <RadioButton
        android:id="@+id/radio_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="461dp"
        tools:ignore="MissingConstraints"/>

    <CheckBox
        android:id="@+id/symptomsLoss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Loss of taste/smell"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="216dp"
        tools:ignore="MissingConstraints"/>

    <CheckBox
        android:id="@+id/symptomsough"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cough"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="168dp"
        tools:ignore="MissingConstraints"/>

    <RadioButton
        android:id="@+id/radioNotfrequent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Not so frequent "
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="547dp"
        tools:ignore="MissingConstraints"/>

    <CheckBox
        android:id="@+id/symptomsFever"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fever"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="258dp"
        tools:layout_editor_absoluteY="169dp"
        tools:ignore="MissingConstraints"/>

    <RadioButton
        android:id="@+id/radioQuitefrequent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quite frequent"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="595dp"
        tools:ignore="MissingConstraints"/>

    <CheckBox
        android:id="@+id/symptomsNone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="None of the above"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="313dp"
        tools:ignore="MissingConstraints"/>

    <RadioButton
        android:id="@+id/radio_yes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yes"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="413dp"
        tools:ignore="MissingConstraints"/>

    <CheckBox
        android:id="@+id/symptomsBreathing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Difficulty in Breathing"
        android:textColor="@color/black"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="264dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/incontact2"
        android:layout_width="384dp"
        android:layout_height="42dp"
        android:text="Frequency in cough?"
        android:textColor="@color/black"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="509dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/incontact"
        android:layout_width="384dp"
        android:layout_height="42dp"
        android:text="Have you been in contact with a covid positive person in last 10-15 days?"
        android:textColor="@color/black"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="368dp"
        tools:ignore="MissingConstraints"/>

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue Assessment"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="682dp"
        tools:ignore="MissingConstraints"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
